//17. Hollow Square Pattern

module tab;
  
  
  initial begin
    

    
    for(int i=0; i<5; i++)begin
      for(int j=0; j<5; j++)begin
        if((i==0 ||i==4 || j==0|| j==4))
          $write("*");
        else 
          $write(" ");
      end
   
   
  
        
      
      $display();
    end
    

    end
    endmodule 
